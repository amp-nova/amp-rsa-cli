  "settings": {
    "label": "Rule Based Content",
    "icons": [
      {
        "size": 256,
        "url": "https://i8.amplience.net/i/willow/CT-icon-style4-usersegment?fmt=jpg&bg=white&w=300"
      }
    ],
    "visualizations": [
      {
        "label": "Sitemap",

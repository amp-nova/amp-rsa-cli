{
  "contentTypeUri": "https://raw.githubusercontent.com/neilmistryamplience/dc-example-website/willow/content-types/curated-product-grid.json",
  "status": "ACTIVE",
  "settings": {
    "label": "Curated Product Grid",
    "icons": [
      {
        "size": 256,
        "url": "https://i8.amplience.net/i/willow/CTicon-style2-CuratedGrid?fmt=jpg&bg=white&w=300"
      }
    ],
    "visualizations": [
      {
        "label": "Website",
        "templatedUri": "{{app.url}}/visualization?vse=\{{vse.domain}}&content=\{{content.sys.id}}&locale=\{{locales}}",
        "default": true
      },
      {
        "label": "QR for Mobile",
        "templatedUri": "{{app.url}}/visualization/qr?vse=\{{vse.domain}}&content=\{{content.sys.id}}&locale=\{{locales}}",
        "default": false
      },
      {
        "label": "JSON",
        "templatedUri": "https://presalesadisws.s3.eu-west-1.amazonaws.com/dynamic-content/dc-visualization-content-delivery/index.html?vse=\{{vse.domain}}&id=\{{content.sys.id}}&format=inlined&depth=all&v2=true&locale=\{{locales}}",
        "default": false
      }
    ],
    "cards": [
      {
        "templatedUri": "https://willow-demo-cards.vercel.app/?#vse=\{{vse.domain}}&content=\{{content.sys.id}}"
      }
    ]
  },
  repositories: [ "content", "contentfr", "contentde", "contentes", "contentit" ]
}